{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "google",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "dateFormat": "readonly",
        "getEndPoint": "readonly",
        "createArticleCard": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": [2, "tab"],
        "no-tabs": "off",
        "max-len": ["error", {"code": 200}],
        "camelcase": ["error", {"allow": ["main_img", "news_id", "src_datetime", "similar_article", "viewed_count"]}]
    }
}